{
  "name": "pheral/essential",
  "description": "The Pheral Essential.",
  "keywords": ["essential", "pheral"],
  "license": "MIT",
  "homepage": "http://pheral.ru",
  "support": {
    "issues": "https://github.com/pheral/essential/issues",
    "source": "https://github.com/pheral/essential"
  },
  "authors": [
    {
      "name": "Dmitriy Yudin",
      "email": "pheral@yandex.ru"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "nesbot/carbon": "~1.22"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.7",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~6.5",
    "phpmd/phpmd": "~2.6",
    "phpunit/phpunit-selenium": "~4.1",
    "squizlabs/php_codesniffer": "3.2"
  },
  "autoload": {
    "psr-4": {
      "Pheral\\Essential\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pheral\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev"
}